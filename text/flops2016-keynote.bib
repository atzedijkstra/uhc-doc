@article{bransen2015phd-ag-incr,
author = {Bransen, J},
title = {{On the Incremental Evaluation of Higher-Order Attribute Grammars}},
journal = {IPA Dissertation Series},
year = {2015},
volume = {2015},
pages = {1--194},
publisher = {Utrecht University},
keywords = {attribute grammars, incremental computation, incremental evaluation, Incrementality},
isbn = {10-1874-313166},
language = {English},
rating = {0},
date-added = {2016-02-19T11:58:38GMT},
date-modified = {2016-02-19T12:00:53GMT},
abstract = {Compilers, amongst other programs, often work with data that (slowly) changes over time. When the changes between subsequent runs of the compiler are small, one would hope the compiler to incrementally update its results, resulting in much lower running time. However, the manual construction of an incremental compiler is hard and error prone and therefore usually not an option.  Attribute grammars provide an attractive way of constructing compilers, as they are compositional in nature and allow for aspect oriented programming. This thesis describes the automatic generation of incremental attribute grammar evaluators, with the purpose of (semi-)automatically generating an incremental compiler from the regular attribute grammar definition. In particular this approach supports incremental evaluation of higher order attributes, a well known extension to the classical attribute grammars that is used in many ways in compiler construction, for example to model different compiler phases.},
url = {http://dspace.library.uu.nl/handle/1874/313166},
local-url = {file://localhost/Volumes/Work/Dropbox/Papers/Library.papers3/Files/E4/E490D383-4E41-4864-A31C-11129D76C6AB.pdf},
file = {{E490D383-4E41-4864-A31C-11129D76C6AB.pdf:/Volumes/Work/Dropbox/Papers/Library.papers3/Files/E4/E490D383-4E41-4864-A31C-11129D76C6AB.pdf:application/pdf;E490D383-4E41-4864-A31C-11129D76C6AB.pdf:/Volumes/Work/Dropbox/Papers/Library.papers3/Files/E4/E490D383-4E41-4864-A31C-11129D76C6AB.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/8252A346-8D93-4D17-816A-8FB7E206FA34}}
}

